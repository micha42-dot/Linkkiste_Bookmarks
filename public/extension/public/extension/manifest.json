{
  "manifest_version": 3,
  "name": "LinkKiste Saver",
  "version": "1.0",
  "description": "Save bookmarks to your private LinkKiste",
  "icons": {
    "48": "icon-48.png"
  },
  "permissions": [
    "activeTab",
    "storage",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Save to LinkKiste"
  }
}